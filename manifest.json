{
  "name": "wind chimes",
  "manifest_version": 2,
  "version": "0.2",
  "description": "A breeze of packets blowing through chimes",
  "browser_action": {
    "default_popup": "volume.html"
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "node_modules/tone/build/Tone.js",
      "chimes.js"
    ],
    "persistent": true
  }
}
